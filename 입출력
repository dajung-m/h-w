#include<cstdio>

int main(){
    printf("Hello World!");
}

@A+B
#include<cstdio>

int A,B; main(){
    scanf("%d%d",&A,&B);
    printf("%d",A+B);
    return 0;
}
@A+B-3
#include<cstdio>
int A,B,T; main(){
    scanf("%d",&T);
    while(T--){
        scanf("%d%d",&A,&B),printf("%d\n",A+B);
    }
    return 0;
}

@A+B EOP
#include<cstdio>
int A,B; main(){
    while(~scanf("%d%d",&A,&B)){
        printf("%d\n",A+B);
    }
    return 0;
}

@A+B ==0
#include<cstdio>
int A,B; main(){
    while(scanf("%d%d",&A,&B)){
        if(!(A+B)) break;
        printf("%d\n",A+B);
    }
    return 0;
}
@A+B A,B
#include<cstdio>
int A,B,T; main(){
    scanf("%d",&T);
    while(T--){
        scanf("%d,%d",&A,&B),printf("%d\n",A+B);
    }
    return 0;
}

@A+B Case #%d: %d\n
#include<cstdio>
int A,B,T,i=0; main(){
    scanf("%d",&T);
    while(T--){
        i++;
        scanf("%d%d",&A,&B),printf("Case #%d: %d\n",i,A+B);
    }
    return 0;
}
